<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LoginInputText" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/black</item>
        <item name="hintTextColor">@color/green</item>
        <item name="cursorColor">#FFFFFF</item>
        <item name="boxStrokeWidth">3dp</item>
    </style>


    <style name="bottomBar.style" parent="Widget.Material3.BottomNavigationView">
        <item name="background">@color/black</item>
        <item name="backgroundTint">@color/black</item>
        <item name="itemIconTint">@color/green</item>
        <item name="itemTextColor">@color/green</item>
        <item name="itemActiveIndicatorStyle">@style/ActiveIndicator</item>
    </style>

    <style name="ActiveIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/gray</item>

    </style>
    


</resources>